{
  "name": "Speech Recognition for Textbox's",
  "short_name": "Speech Recognition for Textbox's",
  "description": "Say \"target input\" to add text into any textbox on the web, and \"clear input\" to clear a textbox",
  "version": "0.0.1",
  "manifest_version": 2,
  "offline_enabled": false,
  "icons": {
    "128": "img/enable.png",
    "64": "img/enable.png",
    "32": "img/enable.png",
    "16": "img/enable.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "img/enable.png",
      "64": "img/enable.png",
      "32": "img/enable.png",
      "16": "img/enable.png"
    },
    "default_title": "Speech Recognition for Textbox's"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [ {
    "js": [ "js/jquery.min.js", "js/annyang.min.js" ],
    "matches": [ "<all_urls>" ],
    "run_at": "document_end"
  } ],
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "update_url": 
